{
  "manifest_version": 3,
  "name": "SUB_CHECK_EXTENSION",
  "version": "1.1",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "static/popup.html",
    "default_icon": "static/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
