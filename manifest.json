{
  "manifest_version": 3,
  "name": "SUB_CHECK_EXTENSION",
  "version": "1.1",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "static/popup.html",
    "default_icon": "static/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
    "js": ["subscriptify_gui.py"]
    }
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
